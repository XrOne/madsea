{
    "nodes": [
        {
            "type": "LoadImage",
            "id": "load_image",
            "params": {
                "path": "${storyboard_image_path}"
            }
        },
        {
            "type": "MiDasDepthPreprocessor",
            "id": "midas_depth",
            "params": {}
        },
        {
            "type": "CannyEdgePreprocessor",
            "id": "canny_edge",
            "params": {
                "low_threshold": 100,
                "high_threshold": 200
            }
        },
        {
            "type": "OpenPosePreprocessor",
            "id": "openpose",
            "params": {}
        },
        {
            "type": "ControlNet",
            "id": "controlnet_depth",
            "params": {
                "model": "diffusion_xl_depth.safetensors",
                "weight": 1.0
            }
        },
        {
            "type": "ControlNet",
            "id": "controlnet_canny",
            "params": {
                "model": "control_sd_xl_canny.safetensors",
                "weight": 1.0
            }
        },
        {
            "type": "ControlNet",
            "id": "controlnet_pose",
            "params": {
                "model": "diffusion_xl_openpose.safetensors",
                "weight": 1.0
            }
        },
        {
            "type": "CheckpointLoader",
            "id": "checkpoint",
            "params": {
                "model": "RealVisXL.safetensors"
            }
        },
        {
            "type": "LoRALoader",
            "id": "lora_silhouette",
            "params": {
                "checkpoint": "silhouette_photographic_style_lora.safetensors",
                "weight": 1.0
            }
        },
        {
            "type": "LoRALoader",
            "id": "lora_rimlight",
            "params": {
                "checkpoint": "rimlight_lora.safetensors",
                "weight": 0.8
            }
        },
        {
            "type": "Prompt",
            "id": "prompt_main",
            "params": {
                "positive": "${prompt_text}",
                "negative": "${negative_prompt}"
            }
        },
        {
            "type": "KSampler",
            "id": "sampler",
            "params": {
                "sampler": "DPM++ 2M Karras",
                "steps": 30,
                "cfg_scale": 8.0,
                "resolution": [1920, 1080],
                "seed": "${seed}"
            }
        },
        {
            "type": "Upscale",
            "id": "upscale",
            "params": {
                "method": "RealESRGAN_x4",
                "scale": 2
            }
        },
        {
            "type": "SaveImage",
            "id": "save_image",
            "params": {
                "path": "${output_path}"
            }
        }
    ],
    "connections": [
        ["load_image", "midas_depth"],
        ["load_image", "canny_edge"],
        ["load_image", "openpose"],
        ["midas_depth", "controlnet_depth"],
        ["canny_edge", "controlnet_canny"],
        ["openpose", "controlnet_pose"],
        ["checkpoint", "sampler"],
        ["lora_silhouette", "sampler"],
        ["lora_rimlight", "sampler"],
        ["prompt_main", "sampler"],
        ["controlnet_depth", "sampler"],
        ["controlnet_canny", "sampler"],
        ["controlnet_pose", "sampler"],
        ["sampler", "upscale"],
        ["upscale", "save_image"]
    ],
    "settings": {
        "cuda_device": "TITAN X Pascal",
        "use_low_vram": true
    }
}
